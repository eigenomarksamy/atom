<?xml version="1.0"?>
<launch>

  <include file="$(find audibot_gazebo)/launch/audibot_named_robot_sens.launch" >
    <arg name="robot_name" default="audibot" />
    <arg name="start_x" value="0" />
    <arg name="start_y" value="0" />
    <arg name="start_z" value="0" />
    <arg name="start_yaw" value="0" />
    <arg name="pub_tf" value="true" />
    <arg name="tf_freq" value="100.0" />
    <arg name="blue" value="false" />
  </include>

  <include file="$(find gazebo_ros)/launch/empty_world.launch" />

  <node pkg="rviz" type="rviz" name="single_vehicle_viz" args="-d $(find audibot_gazebo)/rviz/vehicle_default.rviz" />

  <include file="$(find veh_srv_intf_cfg)/launch/veh_srv_intf_cfg.launch" />
  <include file="$(find veh_srv_gt_odom)/launch/veh_srv_odom_gt.launch" />

</launch>
